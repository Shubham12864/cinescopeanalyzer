{
  "version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://cinescopeanalyzer-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://cinescopeanalyzer-production.up.railway.app",
    "NODE_ENV": "production"
  }
}
